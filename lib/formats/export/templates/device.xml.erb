<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Device' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'type', model: model.fhirType()} %><%- end -%>
    <%- if (!model.note().nil? && !model.note.empty?) -%><%- model.note().each do |element| -%><%== render :template => 'annotation', :locals => {name: 'note', model: element} %><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status %>"<%- if model.has_primitive_extension?('status') -%>><%== model.get_primitive_extension('status').to_xml(is_root: false, is_lowercase: true)  %></status><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.manufacturer().nil? -%><manufacturer value="<%= model.manufacturer %>"<%- if model.has_primitive_extension?('manufacturer') -%>><%== model.get_primitive_extension('manufacturer').to_xml(is_root: false, is_lowercase: true)  %></manufacturer><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.model().nil? -%><model value="<%= model.model %>"<%- if model.has_primitive_extension?('model') -%>><%== model.get_primitive_extension('model').to_xml(is_root: false, is_lowercase: true)  %></model><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.versionNum().nil? -%><version value="<%= model.versionNum %>"<%- if model.has_primitive_extension?('versionNum') -%>><%== model.get_primitive_extension('versionNum').to_xml(is_root: false, is_lowercase: true)  %></version><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.manufactureDate.nil? -%><%- if (!model.manufactureDate.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><manufactureDate value="<%= model.manufactureDate %>"<%- if model.has_primitive_extension?('manufactureDate') -%>><%== model.get_primitive_extension('manufactureDate').to_xml(is_root: false, is_lowercase: true)  %></manufactureDate><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.expiry.nil? -%><%- if (!model.expiry.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><expiry value="<%= model.expiry %>"<%- if model.has_primitive_extension?('expiry') -%>><%== model.get_primitive_extension('expiry').to_xml(is_root: false, is_lowercase: true)  %></expiry><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.udi().nil? -%><udi value="<%= model.udi %>"<%- if model.has_primitive_extension?('udi') -%>><%== model.get_primitive_extension('udi').to_xml(is_root: false, is_lowercase: true)  %></udi><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.lotNumber().nil? -%><lotNumber value="<%= model.lotNumber %>"<%- if model.has_primitive_extension?('lotNumber') -%>><%== model.get_primitive_extension('lotNumber').to_xml(is_root: false, is_lowercase: true)  %></lotNumber><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.owner().nil? -%><%== render :template => 'reference', :locals => {name: 'owner', model: model.owner()} %><%- end -%>
    <%- if !model.location().nil? -%><%== render :template => 'reference', :locals => {name: 'location', model: model.location()} %><%- end -%>
    <%- if !model.patient().nil? -%><%== render :template => 'reference', :locals => {name: 'patient', model: model.patient()} %><%- end -%>
    <%- if (!model.contact().nil? && !model.contact.empty?) -%><%- model.contact().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'contact', model: element} %><%- end -%><%- end -%>
    <%- if !model.url().nil? -%><url value="<%= model.url %>"<%- if model.has_primitive_extension?('url') -%>><%== model.get_primitive_extension('url').to_xml(is_root: false, is_lowercase: true)  %></url><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
