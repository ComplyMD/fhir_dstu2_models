<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Appointment' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status %>"<%- if model.has_primitive_extension?('status') -%>><%== model.get_primitive_extension('status').to_xml(is_root: false, is_lowercase: true)  %></status><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'type', model: model.fhirType()} %><%- end -%>
    <%- if !model.reason().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'reason', model: model.reason()} %><%- end -%>
    <%- if !model.priority().nil? -%><priority value="<%= model.priority %>"<%- if model.has_primitive_extension?('priority') -%>><%== model.get_primitive_extension('priority').to_xml(is_root: false, is_lowercase: true)  %></priority><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description %>"<%- if model.has_primitive_extension?('description') -%>><%== model.get_primitive_extension('description').to_xml(is_root: false, is_lowercase: true)  %></description><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.start.nil? -%><%- if (!model.start.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00)))))\Z/).nil?) -%><start value="<%= model.start %>"<%- if model.has_primitive_extension?('start') -%>><%== model.get_primitive_extension('start').to_xml(is_root: false, is_lowercase: true)  %></start><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.end.nil? -%><%- if (!model.end.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00)))))\Z/).nil?) -%><end value="<%= model.end %>"<%- if model.has_primitive_extension?('end') -%>><%== model.get_primitive_extension('end').to_xml(is_root: false, is_lowercase: true)  %></end><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.minutesDuration().nil? -%><minutesDuration value="<%= model.minutesDuration %>"<%- if model.has_primitive_extension?('minutesDuration') -%>><%== model.get_primitive_extension('minutesDuration').to_xml(is_root: false, is_lowercase: true)  %></minutesDuration><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.slot().nil? && !model.slot.empty?) -%><%- model.slot().each do |element| -%><%== render :template => 'reference', :locals => {name: 'slot', model: element} %><%- end -%><%- end -%>
    <%- if !model.comment().nil? -%><comment value="<%= model.comment %>"<%- if model.has_primitive_extension?('comment') -%>><%== model.get_primitive_extension('comment').to_xml(is_root: false, is_lowercase: true)  %></comment><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.participant().nil? && !model.participant.empty?) -%><%- model.participant().each do |element| -%><%== render :template => 'appointment_appointmentparticipantcomponent', :locals => {name: 'participant', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
