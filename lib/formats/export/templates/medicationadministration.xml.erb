<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'MedicationAdministration' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.patient().nil? -%><%== render :template => 'reference', :locals => {name: 'patient', model: model.patient()} %><%- end -%>
    <%- if !model.practitioner().nil? -%><%== render :template => 'reference', :locals => {name: 'practitioner', model: model.practitioner()} %><%- end -%>
    <%- if !model.encounter().nil? -%><%== render :template => 'reference', :locals => {name: 'encounter', model: model.encounter()} %><%- end -%>
    <%- if !model.prescription().nil? -%><%== render :template => 'reference', :locals => {name: 'prescription', model: model.prescription()} %><%- end -%>
    <%- if !model.wasNotGiven().nil? -%><wasNotGiven value="<%= model.wasNotGiven() %>"/><%- end -%>
    <%- if (!model.reasonNotGiven().nil? && !model.reasonNotGiven.empty?) -%><%- model.reasonNotGiven().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'reasonNotGiven', model: element} %><%- end -%><%- end -%>
    <%- if !model.effectiveTimeDateTime().nil? -%><effectiveTimeDateTime value="<%= model.effectiveTimeDateTime().iso8601 %>"/><%- end -%>
    <%- if !model.effectiveTimePeriod().nil? -%><%== render :template => 'period', :locals => {name: 'effectiveTimePeriod', model: model.effectiveTimePeriod()} %><%- end -%>
    <%- if !model.medication().nil? -%><%== render :template => 'reference', :locals => {name: 'medication', model: model.medication()} %><%- end -%>
    <%- if (!model.device().nil? && !model.device.empty?) -%><%- model.device().each do |element| -%><%== render :template => 'reference', :locals => {name: 'device', model: element} %><%- end -%><%- end -%>
    <%- if (!model.dosage().nil? && !model.dosage.empty?) -%><%- model.dosage().each do |element| -%><%== render :template => 'medicationadministration_medicationadministrationdosagecomponent', :locals => {name: 'dosage', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
