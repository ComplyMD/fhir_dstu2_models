<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.description().nil? -%><description value="<%= model.description() %>"/><%- end -%>
    <%- if !model.during().nil? -%><%== render :template => 'period', :locals => {name: 'during', model: model.during()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
