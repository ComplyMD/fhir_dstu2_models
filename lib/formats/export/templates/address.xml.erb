<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Address' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.use().nil? -%><use value="<%= model.use %>"<%- if model.has_primitive_extension?('use') -%>><%== model.get_primitive_extension('use').to_xml(is_root: false, is_lowercase: true)  %></use><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType %>"<%- if model.has_primitive_extension?('fhirType') -%>><%== model.get_primitive_extension('fhirType').to_xml(is_root: false, is_lowercase: true)  %></type><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.text().nil? -%><text value="<%= model.text %>"<%- if model.has_primitive_extension?('text') -%>><%== model.get_primitive_extension('text').to_xml(is_root: false, is_lowercase: true)  %></text><%- else -%>/><%- end -%><%- end -%>
    <%- if (model.line && !model.line.empty?) -%><%- model.line.each_with_index do |element,index| -%><line value="<%= element %>"<%- if model.has_primitive_extension?('line',index) -%>><%== model.get_primitive_extension('line',index).to_xml(is_root: false, is_lowercase: true)  %></line><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.city().nil? -%><city value="<%= model.city %>"<%- if model.has_primitive_extension?('city') -%>><%== model.get_primitive_extension('city').to_xml(is_root: false, is_lowercase: true)  %></city><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.district().nil? -%><district value="<%= model.district %>"<%- if model.has_primitive_extension?('district') -%>><%== model.get_primitive_extension('district').to_xml(is_root: false, is_lowercase: true)  %></district><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.state().nil? -%><state value="<%= model.state %>"<%- if model.has_primitive_extension?('state') -%>><%== model.get_primitive_extension('state').to_xml(is_root: false, is_lowercase: true)  %></state><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.postalCode().nil? -%><postalCode value="<%= model.postalCode %>"<%- if model.has_primitive_extension?('postalCode') -%>><%== model.get_primitive_extension('postalCode').to_xml(is_root: false, is_lowercase: true)  %></postalCode><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.country().nil? -%><country value="<%= model.country %>"<%- if model.has_primitive_extension?('country') -%>><%== model.get_primitive_extension('country').to_xml(is_root: false, is_lowercase: true)  %></country><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.period().nil? -%><%== render :template => 'period', :locals => {name: 'period', model: model.period()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
