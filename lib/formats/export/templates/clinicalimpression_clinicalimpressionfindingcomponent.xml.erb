<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.item().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'item', model: model.item()} %><%- end -%>
    <%- if !model.cause().nil? -%><cause value="<%= model.cause %>"<%- if model.has_primitive_extension?('cause') -%>><%== model.get_primitive_extension('cause').to_xml(is_root: false, is_lowercase: true)  %></cause><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
