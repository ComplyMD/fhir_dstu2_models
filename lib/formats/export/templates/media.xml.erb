<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Media' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType %>"<%- if model.has_primitive_extension?('fhirType') -%>><%== model.get_primitive_extension('fhirType').to_xml(is_root: false, is_lowercase: true)  %></type><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.subtype().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'subtype', model: model.subtype()} %><%- end -%>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.operator().nil? -%><%== render :template => 'reference', :locals => {name: 'operator', model: model.operator()} %><%- end -%>
    <%- if !model.view().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'view', model: model.view()} %><%- end -%>
    <%- if !model.deviceName().nil? -%><deviceName value="<%= model.deviceName %>"<%- if model.has_primitive_extension?('deviceName') -%>><%== model.get_primitive_extension('deviceName').to_xml(is_root: false, is_lowercase: true)  %></deviceName><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.height().nil? -%><height value="<%= model.height %>"<%- if model.has_primitive_extension?('height') -%>><%== model.get_primitive_extension('height').to_xml(is_root: false, is_lowercase: true)  %></height><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.width().nil? -%><width value="<%= model.width %>"<%- if model.has_primitive_extension?('width') -%>><%== model.get_primitive_extension('width').to_xml(is_root: false, is_lowercase: true)  %></width><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.frames().nil? -%><frames value="<%= model.frames %>"<%- if model.has_primitive_extension?('frames') -%>><%== model.get_primitive_extension('frames').to_xml(is_root: false, is_lowercase: true)  %></frames><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.duration().nil? -%><duration value="<%= model.duration %>"<%- if model.has_primitive_extension?('duration') -%>><%== model.get_primitive_extension('duration').to_xml(is_root: false, is_lowercase: true)  %></duration><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.content().nil? -%><%== render :template => 'attachment', :locals => {name: 'content', model: model.content()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
