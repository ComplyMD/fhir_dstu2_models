<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'OperationDefinition' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.identifier().nil? -%><identifier value="<%= model.identifier() %>"/><%- end -%>
    <%- if !model.versionNum().nil? -%><version value="<%= model.versionNum() %>"/><%- end -%>
    <%- if !model.title().nil? -%><title value="<%= model.title() %>"/><%- end -%>
    <%- if !model.publisher().nil? -%><publisher value="<%= model.publisher() %>"/><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description() %>"/><%- end -%>
    <%- if (!model.code().nil? && !model.code.empty?) -%><%- model.code().each do |element| -%><%== render :template => 'coding', :locals => {name: 'code', model: element} %><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.experimental().nil? -%><experimental value="<%= model.experimental() %>"/><%- end -%>
    <%- if !model.date().nil? -%><date value="<%= model.date().iso8601 %>"/><%- end -%>
    <%- if !model.kind().nil? -%><kind value="<%= model.kind() %>"/><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.notes().nil? -%><notes value="<%= model.notes() %>"/><%- end -%>
    <%- if !model.base().nil? -%><%== render :template => 'reference', :locals => {name: 'base', model: model.base()} %><%- end -%>
    <%- if !model.system().nil? -%><system value="<%= model.system() %>"/><%- end -%>
    <%- if (model.fhirType() && !model.fhirType.empty?) -%><%- model.fhirType().each do |element| -%><type value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.instance().nil? -%><instance value="<%= model.instance() %>"/><%- end -%>
    <%- if (!model.parameter().nil? && !model.parameter.empty?) -%><%- model.parameter().each do |element| -%><%== render :template => 'operationdefinition_operationdefinitionparametercomponent', :locals => {name: 'parameter', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
