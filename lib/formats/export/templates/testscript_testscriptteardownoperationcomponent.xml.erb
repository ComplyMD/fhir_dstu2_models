<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.source().nil? -%><source value="<%= model.source() %>"/><%- end -%>
    <%- if !model.target().nil? -%><target value="<%= model.target() %>"/><%- end -%>
    <%- if !model.destination().nil? -%><destination value="<%= model.destination() %>"/><%- end -%>
    <%- if (model.parameter() && !model.parameter.empty?) -%><%- model.parameter().each do |element| -%><parameter value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.responseId().nil? -%><responseId value="<%= model.responseId() %>"/><%- end -%>
    <%- if !model.contentType().nil? -%><contentType value="<%= model.contentType() %>"/><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
