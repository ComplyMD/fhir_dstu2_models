<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.lotNumber().nil? -%><lotNumber value="<%= model.lotNumber() %>"/><%- end -%>
    <%- if !model.expirationDate().nil? -%><expirationDate value="<%= model.expirationDate().iso8601 %>"/><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
