<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Person' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if (!model.name().nil? && !model.name.empty?) -%><%- model.name().each do |element| -%><%== render :template => 'humanname', :locals => {name: 'name', model: element} %><%- end -%><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.gender().nil? -%><gender value="<%= model.gender() %>"/><%- end -%>
    <%- if !model.birthDate().nil? -%><birthDate value="<%= model.birthDate().iso8601 %>"/><%- end -%>
    <%- if (!model.address().nil? && !model.address.empty?) -%><%- model.address().each do |element| -%><%== render :template => 'address', :locals => {name: 'address', model: element} %><%- end -%><%- end -%>
    <%- if !model.photo().nil? -%><%== render :template => 'attachment', :locals => {name: 'photo', model: model.photo()} %><%- end -%>
    <%- if !model.managingOrganization().nil? -%><%== render :template => 'reference', :locals => {name: 'managingOrganization', model: model.managingOrganization()} %><%- end -%>
    <%- if !model.active().nil? -%><active value="<%= model.active() %>"/><%- end -%>
    <%- if (!model.link().nil? && !model.link.empty?) -%><%- model.link().each do |element| -%><%== render :template => 'person_personlinkcomponent', :locals => {name: 'link', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
