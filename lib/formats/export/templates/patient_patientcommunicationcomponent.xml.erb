<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.language().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'language', model: model.language()} %><%- end -%>
    <%- if !model.preferred().nil? -%><preferred value="<%= model.preferred %>"<%- if model.has_primitive_extension?('preferred') -%>><%== model.get_primitive_extension('preferred').to_xml(is_root: false, is_lowercase: true)  %></preferred><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
