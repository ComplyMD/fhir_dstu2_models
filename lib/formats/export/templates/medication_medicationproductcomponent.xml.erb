<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.form().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'form', model: model.form()} %><%- end -%>
    <%- if (!model.ingredient().nil? && !model.ingredient.empty?) -%><%- model.ingredient().each do |element| -%><%== render :template => 'medication_medicationproductingredientcomponent', :locals => {name: 'ingredient', model: element} %><%- end -%><%- end -%>
    <%- if (!model.batch().nil? && !model.batch.empty?) -%><%- model.batch().each do |element| -%><%== render :template => 'medication_medicationproductbatchcomponent', :locals => {name: 'batch', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
