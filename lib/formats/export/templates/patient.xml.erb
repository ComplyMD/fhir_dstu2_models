<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Patient' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.active().nil? -%><active value="<%= model.active %>"<%- if model.has_primitive_extension?('active') -%>><%== model.get_primitive_extension('active').to_xml(is_root: false, is_lowercase: true)  %></active><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.name().nil? && !model.name.empty?) -%><%- model.name().each do |element| -%><%== render :template => 'humanname', :locals => {name: 'name', model: element} %><%- end -%><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.gender().nil? -%><gender value="<%= model.gender %>"<%- if model.has_primitive_extension?('gender') -%>><%== model.get_primitive_extension('gender').to_xml(is_root: false, is_lowercase: true)  %></gender><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.birthDate.nil? -%><%- if (!model.birthDate.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1]))?)?\Z/).nil?) -%><birthDate value="<%= model.birthDate %>"<%- if model.has_primitive_extension?('birthDate') -%>><%== model.get_primitive_extension('birthDate').to_xml(is_root: false, is_lowercase: true)  %></birthDate><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.deceasedBoolean().nil? -%><deceasedBoolean value="<%= model.deceasedBoolean %>"<%- if model.has_primitive_extension?('deceasedBoolean') -%>><%== model.get_primitive_extension('deceasedBoolean').to_xml(is_root: false, is_lowercase: true)  %></deceasedBoolean><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.deceasedDateTime.nil? -%><%- if (!model.deceasedDateTime.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><deceasedDateTime value="<%= model.deceasedDateTime %>"<%- if model.has_primitive_extension?('deceasedDateTime') -%>><%== model.get_primitive_extension('deceasedDateTime').to_xml(is_root: false, is_lowercase: true)  %></deceasedDateTime><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if (!model.address().nil? && !model.address.empty?) -%><%- model.address().each do |element| -%><%== render :template => 'address', :locals => {name: 'address', model: element} %><%- end -%><%- end -%>
    <%- if !model.maritalStatus().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'maritalStatus', model: model.maritalStatus()} %><%- end -%>
    <%- if !model.multipleBirthBoolean().nil? -%><multipleBirthBoolean value="<%= model.multipleBirthBoolean %>"<%- if model.has_primitive_extension?('multipleBirthBoolean') -%>><%== model.get_primitive_extension('multipleBirthBoolean').to_xml(is_root: false, is_lowercase: true)  %></multipleBirthBoolean><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.multipleBirthInteger().nil? -%><multipleBirthInteger value="<%= model.multipleBirthInteger %>"<%- if model.has_primitive_extension?('multipleBirthInteger') -%>><%== model.get_primitive_extension('multipleBirthInteger').to_xml(is_root: false, is_lowercase: true)  %></multipleBirthInteger><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.photo().nil? && !model.photo.empty?) -%><%- model.photo().each do |element| -%><%== render :template => 'attachment', :locals => {name: 'photo', model: element} %><%- end -%><%- end -%>
    <%- if (!model.contact().nil? && !model.contact.empty?) -%><%- model.contact().each do |element| -%><%== render :template => 'patient_contactcomponent', :locals => {name: 'contact', model: element} %><%- end -%><%- end -%>
    <%- if !model.animal().nil? -%><%== render :template => 'patient_animalcomponent', :locals => {name: 'animal', model: model.animal()} %><%- end -%>
    <%- if (!model.communication().nil? && !model.communication.empty?) -%><%- model.communication().each do |element| -%><%== render :template => 'patient_patientcommunicationcomponent', :locals => {name: 'communication', model: element} %><%- end -%><%- end -%>
    <%- if (!model.careProvider().nil? && !model.careProvider.empty?) -%><%- model.careProvider().each do |element| -%><%== render :template => 'reference', :locals => {name: 'careProvider', model: element} %><%- end -%><%- end -%>
    <%- if !model.managingOrganization().nil? -%><%== render :template => 'reference', :locals => {name: 'managingOrganization', model: model.managingOrganization()} %><%- end -%>
    <%- if (!model.link().nil? && !model.link.empty?) -%><%- model.link().each do |element| -%><%== render :template => 'patient_patientlinkcomponent', :locals => {name: 'link', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
