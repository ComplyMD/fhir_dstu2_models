<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'DeviceComponent' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.fhirType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'type', model: model.fhirType()} %><%- end -%>
    <%- if !model.identifier().nil? -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: model.identifier()} %><%- end -%>
    <%- if !model.lastSystemChange().nil? -%><%- if (!model.lastSystemChange().match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00)))))\Z/).nil?) -%><lastSystemChange value="<%= model.lastSystemChange %>"/><%- end -%><%- end -%>
    <%- if !model.source().nil? -%><%== render :template => 'reference', :locals => {name: 'source', model: model.source()} %><%- end -%>
    <%- if !model.parent().nil? -%><%== render :template => 'reference', :locals => {name: 'parent', model: model.parent()} %><%- end -%>
    <%- if (!model.operationalStatus().nil? && !model.operationalStatus.empty?) -%><%- model.operationalStatus().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'operationalStatus', model: element} %><%- end -%><%- end -%>
    <%- if !model.parameterGroup().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'parameterGroup', model: model.parameterGroup()} %><%- end -%>
    <%- if !model.measurementPrinciple().nil? -%><measurementPrinciple value="<%= model.measurementPrinciple() %>"/><%- end -%>
    <%- if (!model.productionSpecification().nil? && !model.productionSpecification.empty?) -%><%- model.productionSpecification().each do |element| -%><%== render :template => 'devicecomponent_devicecomponentproductionspecificationcomponent', :locals => {name: 'productionSpecification', model: element} %><%- end -%><%- end -%>
    <%- if !model.languageCode().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'languageCode', model: model.languageCode()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
