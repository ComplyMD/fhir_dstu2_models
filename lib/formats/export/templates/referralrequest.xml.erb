<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'ReferralRequest' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.status().nil? -%><status value="<%= model.status %>"<%- if model.has_primitive_extension?('status') -%>><%== model.get_primitive_extension('status').to_xml(is_root: false, is_lowercase: true)  %></status><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.date.nil? -%><%- if (!model.date.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><date value="<%= model.date %>"<%- if model.has_primitive_extension?('date') -%>><%== model.get_primitive_extension('date').to_xml(is_root: false, is_lowercase: true)  %></date><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'type', model: model.fhirType()} %><%- end -%>
    <%- if !model.specialty().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'specialty', model: model.specialty()} %><%- end -%>
    <%- if !model.priority().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'priority', model: model.priority()} %><%- end -%>
    <%- if !model.patient().nil? -%><%== render :template => 'reference', :locals => {name: 'patient', model: model.patient()} %><%- end -%>
    <%- if !model.requester().nil? -%><%== render :template => 'reference', :locals => {name: 'requester', model: model.requester()} %><%- end -%>
    <%- if (!model.recipient().nil? && !model.recipient.empty?) -%><%- model.recipient().each do |element| -%><%== render :template => 'reference', :locals => {name: 'recipient', model: element} %><%- end -%><%- end -%>
    <%- if !model.encounter().nil? -%><%== render :template => 'reference', :locals => {name: 'encounter', model: model.encounter()} %><%- end -%>
    <%- if !model.dateSent.nil? -%><%- if (!model.dateSent.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><dateSent value="<%= model.dateSent %>"<%- if model.has_primitive_extension?('dateSent') -%>><%== model.get_primitive_extension('dateSent').to_xml(is_root: false, is_lowercase: true)  %></dateSent><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.reason().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'reason', model: model.reason()} %><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description %>"<%- if model.has_primitive_extension?('description') -%>><%== model.get_primitive_extension('description').to_xml(is_root: false, is_lowercase: true)  %></description><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.serviceRequested().nil? && !model.serviceRequested.empty?) -%><%- model.serviceRequested().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'serviceRequested', model: element} %><%- end -%><%- end -%>
    <%- if (!model.supportingInformation().nil? && !model.supportingInformation.empty?) -%><%- model.supportingInformation().each do |element| -%><%== render :template => 'reference', :locals => {name: 'supportingInformation', model: element} %><%- end -%><%- end -%>
    <%- if !model.fulfillmentTime().nil? -%><%== render :template => 'period', :locals => {name: 'fulfillmentTime', model: model.fulfillmentTime()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
