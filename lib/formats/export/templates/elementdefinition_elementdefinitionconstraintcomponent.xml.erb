<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.key().nil? -%><key value="<%= model.key %>"<%- if model.has_primitive_extension?('key') -%>><%== model.get_primitive_extension('key').to_xml(is_root: false, is_lowercase: true)  %></key><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.requirements().nil? -%><requirements value="<%= model.requirements %>"<%- if model.has_primitive_extension?('requirements') -%>><%== model.get_primitive_extension('requirements').to_xml(is_root: false, is_lowercase: true)  %></requirements><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.severity().nil? -%><severity value="<%= model.severity %>"<%- if model.has_primitive_extension?('severity') -%>><%== model.get_primitive_extension('severity').to_xml(is_root: false, is_lowercase: true)  %></severity><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.human().nil? -%><human value="<%= model.human %>"<%- if model.has_primitive_extension?('human') -%>><%== model.get_primitive_extension('human').to_xml(is_root: false, is_lowercase: true)  %></human><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.xpath().nil? -%><xpath value="<%= model.xpath %>"<%- if model.has_primitive_extension?('xpath') -%>><%== model.get_primitive_extension('xpath').to_xml(is_root: false, is_lowercase: true)  %></xpath><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
