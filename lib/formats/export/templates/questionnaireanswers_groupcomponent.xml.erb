<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.linkId().nil? -%><linkId value="<%= model.linkId() %>"/><%- end -%>
    <%- if !model.title().nil? -%><title value="<%= model.title() %>"/><%- end -%>
    <%- if !model.text().nil? -%><text value="<%= model.text() %>"/><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if (!model.group().nil? && !model.group.empty?) -%><%- model.group().each do |element| -%><%== render :template => 'questionnaireanswers_groupcomponent', :locals => {name: 'group', model: element} %><%- end -%><%- end -%>
    <%- if (!model.question().nil? && !model.question.empty?) -%><%- model.question().each do |element| -%><%== render :template => 'questionnaireanswers_questioncomponent', :locals => {name: 'question', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
