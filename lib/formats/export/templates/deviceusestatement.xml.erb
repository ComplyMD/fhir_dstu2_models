<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'DeviceUseStatement' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.bodySiteCodeableConcept().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'bodySiteCodeableConcept', model: model.bodySiteCodeableConcept()} %><%- end -%>
    <%- if !model.bodySiteReference().nil? -%><%== render :template => 'reference', :locals => {name: 'bodySiteReference', model: model.bodySiteReference()} %><%- end -%>
    <%- if !model.whenUsed().nil? -%><%== render :template => 'period', :locals => {name: 'whenUsed', model: model.whenUsed()} %><%- end -%>
    <%- if !model.device().nil? -%><%== render :template => 'reference', :locals => {name: 'device', model: model.device()} %><%- end -%>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if (!model.indication().nil? && !model.indication.empty?) -%><%- model.indication().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'indication', model: element} %><%- end -%><%- end -%>
    <%- if (model.notes && !model.notes.empty?) -%><%- model.notes.each_with_index do |element,index| -%><notes value="<%= element %>"<%- if model.has_primitive_extension?('notes',index) -%>><%== model.get_primitive_extension('notes',index).to_xml(is_root: false, is_lowercase: true)  %></notes><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.recordedOn.nil? -%><%- if (!model.recordedOn.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><recordedOn value="<%= model.recordedOn %>"<%- if model.has_primitive_extension?('recordedOn') -%>><%== model.get_primitive_extension('recordedOn').to_xml(is_root: false, is_lowercase: true)  %></recordedOn><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.timingTiming().nil? -%><%== render :template => 'timing', :locals => {name: 'timingTiming', model: model.timingTiming()} %><%- end -%>
    <%- if !model.timingPeriod().nil? -%><%== render :template => 'period', :locals => {name: 'timingPeriod', model: model.timingPeriod()} %><%- end -%>
    <%- if !model.timingDateTime.nil? -%><%- if (!model.timingDateTime.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><timingDateTime value="<%= model.timingDateTime %>"<%- if model.has_primitive_extension?('timingDateTime') -%>><%== model.get_primitive_extension('timingDateTime').to_xml(is_root: false, is_lowercase: true)  %></timingDateTime><%- else -%>/><%- end -%><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
