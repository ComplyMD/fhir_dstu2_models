<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.purpose().nil? -%><purpose value="<%= model.purpose %>"<%- if model.has_primitive_extension?('purpose') -%>><%== model.get_primitive_extension('purpose').to_xml(is_root: false, is_lowercase: true)  %></purpose><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name %>"<%- if model.has_primitive_extension?('name') -%>><%== model.get_primitive_extension('name').to_xml(is_root: false, is_lowercase: true)  %></name><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description %>"<%- if model.has_primitive_extension?('description') -%>><%== model.get_primitive_extension('description').to_xml(is_root: false, is_lowercase: true)  %></description><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.acronym().nil? -%><acronym value="<%= model.acronym %>"<%- if model.has_primitive_extension?('acronym') -%>><%== model.get_primitive_extension('acronym').to_xml(is_root: false, is_lowercase: true)  %></acronym><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.sourceUri().nil? -%><sourceUri value="<%= model.sourceUri %>"<%- if model.has_primitive_extension?('sourceUri') -%>><%== model.get_primitive_extension('sourceUri').to_xml(is_root: false, is_lowercase: true)  %></sourceUri><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.sourceReference().nil? -%><%== render :template => 'reference', :locals => {name: 'sourceReference', model: model.sourceReference()} %><%- end -%>
    <%- if !model.exampleFor().nil? -%><%== render :template => 'reference', :locals => {name: 'exampleFor', model: model.exampleFor()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
