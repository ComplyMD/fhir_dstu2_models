<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.attachment().nil? -%><%== render :template => 'attachment', :locals => {name: 'attachment', model: model.attachment()} %><%- end -%>
    <%- if (!model.format().nil? && !model.format.empty?) -%><%- model.format().each do |element| -%><%== render :template => 'coding', :locals => {name: 'format', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
