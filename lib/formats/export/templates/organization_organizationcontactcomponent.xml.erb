<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.purpose().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'purpose', model: model.purpose()} %><%- end -%>
    <%- if !model.name().nil? -%><%== render :template => 'humanname', :locals => {name: 'name', model: model.name()} %><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.address().nil? -%><%== render :template => 'address', :locals => {name: 'address', model: model.address()} %><%- end -%>
    <%- if !model.gender().nil? -%><gender value="<%= model.gender() %>"/><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
