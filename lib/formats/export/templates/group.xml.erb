<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Group' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType %>"<%- if model.has_primitive_extension?('fhirType') -%>><%== model.get_primitive_extension('fhirType').to_xml(is_root: false, is_lowercase: true)  %></type><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.actual().nil? -%><actual value="<%= model.actual %>"<%- if model.has_primitive_extension?('actual') -%>><%== model.get_primitive_extension('actual').to_xml(is_root: false, is_lowercase: true)  %></actual><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.code().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'code', model: model.code()} %><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name %>"<%- if model.has_primitive_extension?('name') -%>><%== model.get_primitive_extension('name').to_xml(is_root: false, is_lowercase: true)  %></name><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.quantity().nil? -%><quantity value="<%= model.quantity %>"<%- if model.has_primitive_extension?('quantity') -%>><%== model.get_primitive_extension('quantity').to_xml(is_root: false, is_lowercase: true)  %></quantity><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.characteristic().nil? && !model.characteristic.empty?) -%><%- model.characteristic().each do |element| -%><%== render :template => 'group_groupcharacteristiccomponent', :locals => {name: 'characteristic', model: element} %><%- end -%><%- end -%>
    <%- if (!model.member().nil? && !model.member.empty?) -%><%- model.member().each do |element| -%><%== render :template => 'group_groupmembercomponent', :locals => {name: 'member', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
