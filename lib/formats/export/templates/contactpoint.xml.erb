<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'ContactPoint' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.system().nil? -%><system value="<%= model.system %>"<%- if model.has_primitive_extension?('system') -%>><%== model.get_primitive_extension('system').to_xml(is_root: false, is_lowercase: true)  %></system><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.value().nil? -%><value value="<%= model.value %>"<%- if model.has_primitive_extension?('value') -%>><%== model.get_primitive_extension('value').to_xml(is_root: false, is_lowercase: true)  %></value><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.use().nil? -%><use value="<%= model.use %>"<%- if model.has_primitive_extension?('use') -%>><%== model.get_primitive_extension('use').to_xml(is_root: false, is_lowercase: true)  %></use><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.rank().nil? -%><rank value="<%= model.rank %>"<%- if model.has_primitive_extension?('rank') -%>><%== model.get_primitive_extension('rank').to_xml(is_root: false, is_lowercase: true)  %></rank><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.period().nil? -%><%== render :template => 'period', :locals => {name: 'period', model: model.period()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
