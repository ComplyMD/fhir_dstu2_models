<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.product().nil? -%><%== render :template => 'coding', :locals => {name: 'product', model: model.product()} %><%- end -%>
    <%- if !model.eye().nil? -%><eye value="<%= model.eye %>"<%- if model.has_primitive_extension?('eye') -%>><%== model.get_primitive_extension('eye').to_xml(is_root: false, is_lowercase: true)  %></eye><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.sphere().nil? -%><sphere value="<%= model.sphere %>"<%- if model.has_primitive_extension?('sphere') -%>><%== model.get_primitive_extension('sphere').to_xml(is_root: false, is_lowercase: true)  %></sphere><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.cylinder().nil? -%><cylinder value="<%= model.cylinder %>"<%- if model.has_primitive_extension?('cylinder') -%>><%== model.get_primitive_extension('cylinder').to_xml(is_root: false, is_lowercase: true)  %></cylinder><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.axis().nil? -%><axis value="<%= model.axis %>"<%- if model.has_primitive_extension?('axis') -%>><%== model.get_primitive_extension('axis').to_xml(is_root: false, is_lowercase: true)  %></axis><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.prism().nil? -%><prism value="<%= model.prism %>"<%- if model.has_primitive_extension?('prism') -%>><%== model.get_primitive_extension('prism').to_xml(is_root: false, is_lowercase: true)  %></prism><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.base().nil? -%><base value="<%= model.base %>"<%- if model.has_primitive_extension?('base') -%>><%== model.get_primitive_extension('base').to_xml(is_root: false, is_lowercase: true)  %></base><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.add().nil? -%><add value="<%= model.add %>"<%- if model.has_primitive_extension?('add') -%>><%== model.get_primitive_extension('add').to_xml(is_root: false, is_lowercase: true)  %></add><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.power().nil? -%><power value="<%= model.power %>"<%- if model.has_primitive_extension?('power') -%>><%== model.get_primitive_extension('power').to_xml(is_root: false, is_lowercase: true)  %></power><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.backCurve().nil? -%><backCurve value="<%= model.backCurve %>"<%- if model.has_primitive_extension?('backCurve') -%>><%== model.get_primitive_extension('backCurve').to_xml(is_root: false, is_lowercase: true)  %></backCurve><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.diameter().nil? -%><diameter value="<%= model.diameter %>"<%- if model.has_primitive_extension?('diameter') -%>><%== model.get_primitive_extension('diameter').to_xml(is_root: false, is_lowercase: true)  %></diameter><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.duration().nil? -%><%== render :template => 'quantity', :locals => {name: 'duration', model: model.duration()} %><%- end -%>
    <%- if !model.color().nil? -%><color value="<%= model.color %>"<%- if model.has_primitive_extension?('color') -%>><%== model.get_primitive_extension('color').to_xml(is_root: false, is_lowercase: true)  %></color><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.brand().nil? -%><brand value="<%= model.brand %>"<%- if model.has_primitive_extension?('brand') -%>><%== model.get_primitive_extension('brand').to_xml(is_root: false, is_lowercase: true)  %></brand><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.notes().nil? -%><notes value="<%= model.notes %>"<%- if model.has_primitive_extension?('notes') -%>><%== model.get_primitive_extension('notes').to_xml(is_root: false, is_lowercase: true)  %></notes><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
