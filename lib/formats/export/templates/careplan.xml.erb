<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'CarePlan' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status %>"<%- if model.has_primitive_extension?('status') -%>><%== model.get_primitive_extension('status').to_xml(is_root: false, is_lowercase: true)  %></status><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.context().nil? -%><%== render :template => 'reference', :locals => {name: 'context', model: model.context()} %><%- end -%>
    <%- if !model.period().nil? -%><%== render :template => 'period', :locals => {name: 'period', model: model.period()} %><%- end -%>
    <%- if (!model.author().nil? && !model.author.empty?) -%><%- model.author().each do |element| -%><%== render :template => 'reference', :locals => {name: 'author', model: element} %><%- end -%><%- end -%>
    <%- if !model.modified.nil? -%><%- if (!model.modified.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><modified value="<%= model.modified %>"<%- if model.has_primitive_extension?('modified') -%>><%== model.get_primitive_extension('modified').to_xml(is_root: false, is_lowercase: true)  %></modified><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if (!model.category().nil? && !model.category.empty?) -%><%- model.category().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'category', model: element} %><%- end -%><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description %>"<%- if model.has_primitive_extension?('description') -%>><%== model.get_primitive_extension('description').to_xml(is_root: false, is_lowercase: true)  %></description><%- else -%>/><%- end -%><%- end -%>
    <%- if (!model.addresses().nil? && !model.addresses.empty?) -%><%- model.addresses().each do |element| -%><%== render :template => 'reference', :locals => {name: 'addresses', model: element} %><%- end -%><%- end -%>
    <%- if (!model.support().nil? && !model.support.empty?) -%><%- model.support().each do |element| -%><%== render :template => 'reference', :locals => {name: 'support', model: element} %><%- end -%><%- end -%>
    <%- if (!model.relatedPlan().nil? && !model.relatedPlan.empty?) -%><%- model.relatedPlan().each do |element| -%><%== render :template => 'careplan_careplanrelatedplancomponent', :locals => {name: 'relatedPlan', model: element} %><%- end -%><%- end -%>
    <%- if (!model.participant().nil? && !model.participant.empty?) -%><%- model.participant().each do |element| -%><%== render :template => 'careplan_careplanparticipantcomponent', :locals => {name: 'participant', model: element} %><%- end -%><%- end -%>
    <%- if (!model.goal().nil? && !model.goal.empty?) -%><%- model.goal().each do |element| -%><%== render :template => 'reference', :locals => {name: 'goal', model: element} %><%- end -%><%- end -%>
    <%- if (!model.activity().nil? && !model.activity.empty?) -%><%- model.activity().each do |element| -%><%== render :template => 'careplan_careplanactivitycomponent', :locals => {name: 'activity', model: element} %><%- end -%><%- end -%>
    <%- if !model.note().nil? -%><%== render :template => 'annotation', :locals => {name: 'note', model: model.note()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
