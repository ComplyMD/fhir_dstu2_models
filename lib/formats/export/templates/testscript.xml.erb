<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'TestScript' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description() %>"/><%- end -%>
    <%- if (!model.fixture().nil? && !model.fixture.empty?) -%><%- model.fixture().each do |element| -%><%== render :template => 'testscript_testscriptfixturecomponent', :locals => {name: 'fixture', model: element} %><%- end -%><%- end -%>
    <%- if !model.setup().nil? -%><%== render :template => 'testscript_testscriptsetupcomponent', :locals => {name: 'setup', model: model.setup()} %><%- end -%>
    <%- if (!model.test().nil? && !model.test.empty?) -%><%- model.test().each do |element| -%><%== render :template => 'testscript_testscripttestcomponent', :locals => {name: 'test', model: element} %><%- end -%><%- end -%>
    <%- if !model.teardown().nil? -%><%== render :template => 'testscript_testscriptteardowncomponent', :locals => {name: 'teardown', model: model.teardown()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
