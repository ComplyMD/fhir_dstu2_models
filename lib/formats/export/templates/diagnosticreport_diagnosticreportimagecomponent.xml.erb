<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.comment().nil? -%><comment value="<%= model.comment %>"<%- if model.has_primitive_extension?('comment') -%>><%== model.get_primitive_extension('comment').to_xml(is_root: false, is_lowercase: true)  %></comment><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.link().nil? -%><%== render :template => 'reference', :locals => {name: 'link', model: model.link()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
