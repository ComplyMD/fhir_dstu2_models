<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'DataElement' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.identifier().nil? -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: model.identifier()} %><%- end -%>
    <%- if !model.versionNum().nil? -%><version value="<%= model.versionNum() %>"/><%- end -%>
    <%- if !model.publisher().nil? -%><publisher value="<%= model.publisher() %>"/><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contactpoint', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.date().nil? -%><date value="<%= model.date().iso8601 %>"/><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if (!model.category().nil? && !model.category.empty?) -%><%- model.category().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'category', model: element} %><%- end -%><%- end -%>
    <%- if !model.granularity().nil? -%><granularity value="<%= model.granularity() %>"/><%- end -%>
    <%- if (!model.code().nil? && !model.code.empty?) -%><%- model.code().each do |element| -%><%== render :template => 'coding', :locals => {name: 'code', model: element} %><%- end -%><%- end -%>
    <%- if !model.question().nil? -%><question value="<%= model.question() %>"/><%- end -%>
    <%- if !model.label().nil? -%><label value="<%= model.label() %>"/><%- end -%>
    <%- if !model.definition().nil? -%><definition value="<%= model.definition() %>"/><%- end -%>
    <%- if !model.comments().nil? -%><comments value="<%= model.comments() %>"/><%- end -%>
    <%- if !model.requirements().nil? -%><requirements value="<%= model.requirements() %>"/><%- end -%>
    <%- if (model.synonym() && !model.synonym.empty?) -%><%- model.synonym().each do |element| -%><synonym value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.example().nil? -%><%- if FHIR::AnyType::PRIMITIVES.include? model.exampleType().downcase -%><example<%= model.exampleType() %> value="<%= model.example() %>"/><%- elsif FHIR::AnyType::DATE_TIMES.include? model.exampleType().downcase -%><example<%= model.exampleType() %> value="<%= model.example().iso8601 %>"/><%- else -%><%== model.example().to_xml(is_root: false, name: "example#{model.exampleType()}")%><%- end -%><%- end -%>
    <%- if !model.maxLength().nil? -%><maxLength value="<%= model.maxLength() %>"/><%- end -%>
    <%- if !model.unitsCodeableConcept().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'unitsCodeableConcept', model: model.unitsCodeableConcept()} %><%- end -%>
    <%- if !model.unitsReference().nil? -%><%== render :template => 'reference', :locals => {name: 'unitsReference', model: model.unitsReference()} %><%- end -%>
    <%- if !model.binding().nil? -%><%== render :template => 'dataelement_dataelementbindingcomponent', :locals => {name: 'binding', model: model.binding()} %><%- end -%>
    <%- if (!model.mapping().nil? && !model.mapping.empty?) -%><%- model.mapping().each do |element| -%><%== render :template => 'dataelement_dataelementmappingcomponent', :locals => {name: 'mapping', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
