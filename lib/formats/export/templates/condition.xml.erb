<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Condition' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.patient().nil? -%><%== render :template => 'reference', :locals => {name: 'patient', model: model.patient()} %><%- end -%>
    <%- if !model.encounter().nil? -%><%== render :template => 'reference', :locals => {name: 'encounter', model: model.encounter()} %><%- end -%>
    <%- if !model.asserter().nil? -%><%== render :template => 'reference', :locals => {name: 'asserter', model: model.asserter()} %><%- end -%>
    <%- if !model.dateRecorded.nil? -%><%- if (!model.dateRecorded.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1]))?)?\Z/).nil?) -%><dateRecorded value="<%= model.dateRecorded %>"<%- if model.has_primitive_extension?('dateRecorded') -%>><%== model.get_primitive_extension('dateRecorded').to_xml(is_root: false, is_lowercase: true)  %></dateRecorded><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.code().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'code', model: model.code()} %><%- end -%>
    <%- if !model.category().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'category', model: model.category()} %><%- end -%>
    <%- if !model.clinicalStatus().nil? -%><clinicalStatus value="<%= model.clinicalStatus %>"<%- if model.has_primitive_extension?('clinicalStatus') -%>><%== model.get_primitive_extension('clinicalStatus').to_xml(is_root: false, is_lowercase: true)  %></clinicalStatus><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.verificationStatus().nil? -%><verificationStatus value="<%= model.verificationStatus %>"<%- if model.has_primitive_extension?('verificationStatus') -%>><%== model.get_primitive_extension('verificationStatus').to_xml(is_root: false, is_lowercase: true)  %></verificationStatus><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.severity().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'severity', model: model.severity()} %><%- end -%>
    <%- if !model.onsetDateTime.nil? -%><%- if (!model.onsetDateTime.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><onsetDateTime value="<%= model.onsetDateTime %>"<%- if model.has_primitive_extension?('onsetDateTime') -%>><%== model.get_primitive_extension('onsetDateTime').to_xml(is_root: false, is_lowercase: true)  %></onsetDateTime><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.onsetQuantity().nil? -%><%== render :template => 'quantity', :locals => {name: 'onsetQuantity', model: model.onsetQuantity()} %><%- end -%>
    <%- if !model.onsetPeriod().nil? -%><%== render :template => 'period', :locals => {name: 'onsetPeriod', model: model.onsetPeriod()} %><%- end -%>
    <%- if !model.onsetRange().nil? -%><%== render :template => 'range', :locals => {name: 'onsetRange', model: model.onsetRange()} %><%- end -%>
    <%- if !model.onsetString().nil? -%><onsetString value="<%= model.onsetString %>"<%- if model.has_primitive_extension?('onsetString') -%>><%== model.get_primitive_extension('onsetString').to_xml(is_root: false, is_lowercase: true)  %></onsetString><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.abatementDateTime.nil? -%><%- if (!model.abatementDateTime.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><abatementDateTime value="<%= model.abatementDateTime %>"<%- if model.has_primitive_extension?('abatementDateTime') -%>><%== model.get_primitive_extension('abatementDateTime').to_xml(is_root: false, is_lowercase: true)  %></abatementDateTime><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.abatementQuantity().nil? -%><%== render :template => 'quantity', :locals => {name: 'abatementQuantity', model: model.abatementQuantity()} %><%- end -%>
    <%- if !model.abatementBoolean().nil? -%><abatementBoolean value="<%= model.abatementBoolean %>"<%- if model.has_primitive_extension?('abatementBoolean') -%>><%== model.get_primitive_extension('abatementBoolean').to_xml(is_root: false, is_lowercase: true)  %></abatementBoolean><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.abatementPeriod().nil? -%><%== render :template => 'period', :locals => {name: 'abatementPeriod', model: model.abatementPeriod()} %><%- end -%>
    <%- if !model.abatementRange().nil? -%><%== render :template => 'range', :locals => {name: 'abatementRange', model: model.abatementRange()} %><%- end -%>
    <%- if !model.abatementString().nil? -%><abatementString value="<%= model.abatementString %>"<%- if model.has_primitive_extension?('abatementString') -%>><%== model.get_primitive_extension('abatementString').to_xml(is_root: false, is_lowercase: true)  %></abatementString><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.stage().nil? -%><%== render :template => 'condition_conditionstagecomponent', :locals => {name: 'stage', model: model.stage()} %><%- end -%>
    <%- if (!model.evidence().nil? && !model.evidence.empty?) -%><%- model.evidence().each do |element| -%><%== render :template => 'condition_conditionevidencecomponent', :locals => {name: 'evidence', model: element} %><%- end -%><%- end -%>
    <%- if (!model.bodySite().nil? && !model.bodySite.empty?) -%><%- model.bodySite().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'bodySite', model: element} %><%- end -%><%- end -%>
    <%- if !model.notes().nil? -%><notes value="<%= model.notes %>"<%- if model.has_primitive_extension?('notes') -%>><%== model.get_primitive_extension('notes').to_xml(is_root: false, is_lowercase: true)  %></notes><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
