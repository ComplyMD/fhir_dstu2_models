<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.code().nil? -%><%== render :template => 'coding', :locals => {name: 'code', model: model.code()} %><%- end -%>
    <%- if !model.amount().nil? -%><%== render :template => 'quantity', :locals => {name: 'amount', model: model.amount()} %><%- end -%>
    <%- if !model.value().nil? -%><value value="<%= model.value %>"<%- if model.has_primitive_extension?('value') -%>><%== model.get_primitive_extension('value').to_xml(is_root: false, is_lowercase: true)  %></value><%- else -%>/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
