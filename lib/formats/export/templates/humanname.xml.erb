<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'HumanName' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.use().nil? -%><use value="<%= model.use %>"<%- if model.has_primitive_extension?('use') -%>><%== model.get_primitive_extension('use').to_xml(is_root: false, is_lowercase: true)  %></use><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.text().nil? -%><text value="<%= model.text %>"<%- if model.has_primitive_extension?('text') -%>><%== model.get_primitive_extension('text').to_xml(is_root: false, is_lowercase: true)  %></text><%- else -%>/><%- end -%><%- end -%>
    <%- if (model.family && !model.family.empty?) -%><%- model.family.each_with_index do |element,index| -%><family value="<%= element %>"<%- if model.has_primitive_extension?('family',index) -%>><%== model.get_primitive_extension('family',index).to_xml(is_root: false, is_lowercase: true)  %></family><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if (model.given && !model.given.empty?) -%><%- model.given.each_with_index do |element,index| -%><given value="<%= element %>"<%- if model.has_primitive_extension?('given',index) -%>><%== model.get_primitive_extension('given',index).to_xml(is_root: false, is_lowercase: true)  %></given><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if (model.prefix && !model.prefix.empty?) -%><%- model.prefix.each_with_index do |element,index| -%><prefix value="<%= element %>"<%- if model.has_primitive_extension?('prefix',index) -%>><%== model.get_primitive_extension('prefix',index).to_xml(is_root: false, is_lowercase: true)  %></prefix><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if (model.suffix && !model.suffix.empty?) -%><%- model.suffix.each_with_index do |element,index| -%><suffix value="<%= element %>"<%- if model.has_primitive_extension?('suffix',index) -%>><%== model.get_primitive_extension('suffix',index).to_xml(is_root: false, is_lowercase: true)  %></suffix><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.period().nil? -%><%== render :template => 'period', :locals => {name: 'period', model: model.period()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
