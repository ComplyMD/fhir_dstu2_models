<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'AllergyIntolerance' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.recordedDate().nil? -%><recordedDate value="<%= model.recordedDate().iso8601 %>"/><%- end -%>
    <%- if !model.recorder().nil? -%><%== render :template => 'reference', :locals => {name: 'recorder', model: model.recorder()} %><%- end -%>
    <%- if !model.patient().nil? -%><%== render :template => 'reference', :locals => {name: 'patient', model: model.patient()} %><%- end -%>
    <%- if !model.reporter().nil? -%><%== render :template => 'reference', :locals => {name: 'reporter', model: model.reporter()} %><%- end -%>
    <%- if !model.substance().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'substance', model: model.substance()} %><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.criticality().nil? -%><criticality value="<%= model.criticality() %>"/><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.category().nil? -%><category value="<%= model.category() %>"/><%- end -%>
    <%- if !model.lastOccurence().nil? -%><lastOccurence value="<%= model.lastOccurence().iso8601 %>"/><%- end -%>
    <%- if !model.comment().nil? -%><comment value="<%= model.comment() %>"/><%- end -%>
    <%- if (!model.event().nil? && !model.event.empty?) -%><%- model.event().each do |element| -%><%== render :template => 'allergyintolerance_allergyintoleranceeventcomponent', :locals => {name: 'event', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
