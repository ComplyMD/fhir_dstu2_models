<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Media' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.subtype().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'subtype', model: model.subtype()} %><%- end -%>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.operator().nil? -%><%== render :template => 'reference', :locals => {name: 'operator', model: model.operator()} %><%- end -%>
    <%- if !model.view().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'view', model: model.view()} %><%- end -%>
    <%- if !model.deviceName().nil? -%><deviceName value="<%= model.deviceName() %>"/><%- end -%>
    <%- if !model.height().nil? -%><height value="<%= model.height() %>"/><%- end -%>
    <%- if !model.width().nil? -%><width value="<%= model.width() %>"/><%- end -%>
    <%- if !model.frames().nil? -%><frames value="<%= model.frames() %>"/><%- end -%>
    <%- if !model.duration().nil? -%><duration value="<%= model.duration() %>"/><%- end -%>
    <%- if !model.content().nil? -%><%== render :template => 'attachment', :locals => {name: 'content', model: model.content()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
