<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if (model.daysOfWeek() && !model.daysOfWeek.empty?) -%><%- model.daysOfWeek().each do |element| -%><daysOfWeek value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.allDay().nil? -%><allDay value="<%= model.allDay() %>"/><%- end -%>
    <%- if !model.availableStartTime().nil? -%><%- if (!model.availableStartTime().match(/\A([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?\Z/).nil?) -%><availableStartTime value="<%= model.availableStartTime %>"/><%- end -%><%- end -%>
    <%- if !model.availableEndTime().nil? -%><%- if (!model.availableEndTime().match(/\A([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?\Z/).nil?) -%><availableEndTime value="<%= model.availableEndTime %>"/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
