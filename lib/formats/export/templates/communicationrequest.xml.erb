<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'CommunicationRequest' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.category().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'category', model: model.category()} %><%- end -%>
    <%- if !model.sender().nil? -%><%== render :template => 'reference', :locals => {name: 'sender', model: model.sender()} %><%- end -%>
    <%- if (!model.recipient().nil? && !model.recipient.empty?) -%><%- model.recipient().each do |element| -%><%== render :template => 'reference', :locals => {name: 'recipient', model: element} %><%- end -%><%- end -%>
    <%- if (!model.payload().nil? && !model.payload.empty?) -%><%- model.payload().each do |element| -%><%== render :template => 'communicationrequest_communicationrequestpayloadcomponent', :locals => {name: 'payload', model: element} %><%- end -%><%- end -%>
    <%- if (!model.medium().nil? && !model.medium.empty?) -%><%- model.medium().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'medium', model: element} %><%- end -%><%- end -%>
    <%- if !model.requester().nil? -%><%== render :template => 'reference', :locals => {name: 'requester', model: model.requester()} %><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status %>"<%- if model.has_primitive_extension?('status') -%>><%== model.get_primitive_extension('status').to_xml(is_root: false, is_lowercase: true)  %></status><%- else -%>/><%- end -%><%- end -%>
    <%- if !model.encounter().nil? -%><%== render :template => 'reference', :locals => {name: 'encounter', model: model.encounter()} %><%- end -%>
    <%- if !model.scheduledDateTime.nil? -%><%- if (!model.scheduledDateTime.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><scheduledDateTime value="<%= model.scheduledDateTime %>"<%- if model.has_primitive_extension?('scheduledDateTime') -%>><%== model.get_primitive_extension('scheduledDateTime').to_xml(is_root: false, is_lowercase: true)  %></scheduledDateTime><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.scheduledPeriod().nil? -%><%== render :template => 'period', :locals => {name: 'scheduledPeriod', model: model.scheduledPeriod()} %><%- end -%>
    <%- if (!model.reason().nil? && !model.reason.empty?) -%><%- model.reason().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'reason', model: element} %><%- end -%><%- end -%>
    <%- if !model.requestedOn.nil? -%><%- if (!model.requestedOn.match(/\A[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\.[0-9]+)?(Z|(\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?)?)?)?\Z/).nil?) -%><requestedOn value="<%= model.requestedOn %>"<%- if model.has_primitive_extension?('requestedOn') -%>><%== model.get_primitive_extension('requestedOn').to_xml(is_root: false, is_lowercase: true)  %></requestedOn><%- else -%>/><%- end -%><%- end -%><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'reference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.priority().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'priority', model: model.priority()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
