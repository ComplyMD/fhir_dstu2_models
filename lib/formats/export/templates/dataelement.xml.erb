<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'DataElement' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: true} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.url().nil? -%><url value="<%= model.url() %>"/><%- end -%>
    <%- if !model.identifier().nil? -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: model.identifier()} %><%- end -%>
    <%- if !model.versionNum().nil? -%><version value="<%= model.versionNum() %>"/><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if (!model.useContext().nil? && !model.useContext.empty?) -%><%- model.useContext().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'useContext', model: element} %><%- end -%><%- end -%>
    <%- if !model.experimental().nil? -%><experimental value="<%= model.experimental() %>"/><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.date().nil? -%><date value="<%= model.date().to_date.iso8601 %>"/><%- end -%>
    <%- if !model.copyright().nil? -%><copyright value="<%= model.copyright() %>"/><%- end -%>
    <%- if !model.publisher().nil? -%><publisher value="<%= model.publisher() %>"/><%- end -%>
    <%- if (!model.contact().nil? && !model.contact.empty?) -%><%- model.contact().each do |element| -%><%== render :template => 'dataelement_dataelementcontactcomponent', :locals => {name: 'contact', model: element} %><%- end -%><%- end -%>
    <%- if !model.specificity().nil? -%><specificity value="<%= model.specificity() %>"/><%- end -%>
    <%- if (!model.mapping().nil? && !model.mapping.empty?) -%><%- model.mapping().each do |element| -%><%== render :template => 'dataelement_dataelementmappingcomponent', :locals => {name: 'mapping', model: element} %><%- end -%><%- end -%>
    <%- if (!model.element().nil? && !model.element.empty?) -%><%- model.element().each do |element| -%><%== render :template => 'elementdefinition', :locals => {name: 'element', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
